# Future Stack Environment Configuration
# Copy this file to .env and fill in your values
# DO NOT commit the actual .env file to version control

# ===== General Configuration =====
NODE_ENV=development
DEBUG=false

# ===== Web Application =====
WEB_PORT=3000
WEB_HOST=localhost

# ===== Database Configuration =====
DB_TYPE=postgres
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=changeme
DB_NAME=future_stack
DB_SSL=false

# ===== Traefik Configuration =====
TRAEFIK_DASHBOARD_ENABLED=false
TRAEFIK_DASHBOARD_PORT=8080
TRAEFIK_ACME_EMAIL=example@example.com
TRAEFIK_LOG_LEVEL=ERROR
DOMAIN=localhost

# ===== Coolify Configuration =====
COOLIFY_SECRET_KEY=changeme
COOLIFY_PUBLIC_IP=127.0.0.1
COOLIFY_HOSTED_ON=docker
COOLIFY_WHITE_LABELED=false

# ===== Security =====
JWT_SECRET=changeme
SESSION_SECRET=changeme
COOKIE_SECRET=changeme

# ===== External Services =====
# Only fill these in if you're using the respective services
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# ===== RabbitHoles Service =====
RABBITHOLES_BUILD_CONTEXT=../path/to/your/rabbitholes/source
RABBITHOLES_DOCKERFILE=Dockerfile
RABBITHOLES_CONTAINER_NAME=rabbitholes
RABBITHOLES_PORT=3001
RABBITHOLES_DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@postgres:5432/${POSTGRES_DB_VECTOR}?sslmode=disable
TAVILY_API_KEY=
GOOGLE_AI_API_KEY=
GOOGLE_AI_MODEL=

# ===== Postgres Vector DB (for RabbitHoles) =====
POSTGRES_USER=postgres
POSTGRES_PASSWORD=changeme
POSTGRES_DB_VECTOR=future_stack_vector